@{
    ViewData["Title"] = "Admin - UsExpress";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var UserId = (string)ViewBag.UserId;

}
<div class="row-main container flex">
    <div class="row-main-left">
        <div class="wrap_bg mb20 thong_ke_top">
            @if (UserId != "62")
            {
                <h2 class="title_cate">Kết quả bán hàng hôm nay</h2>
                <div class="row">
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/RevenuToday.cshtml" />
                    </div>
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/RevenuTemp.cshtml" />
                    </div>
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/CustomerInDay.cshtml" />
                    </div>
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/RevenuDay.cshtml" />
                    </div>
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/CrawlPercent.cshtml" />
                    </div>
                    <div class="col-md-2 col-6">
                        <partial name="~/Views/DashBoard/ErrorOrder.cshtml" />
                    </div>
                </div>
            }
        </div>
        <div class="wrap_bg mb20">
            <partial name="~/Views/DashBoard/ChartRevenu.cshtml" />
        </div>
        <div class="wrap_bg mb20">
            <partial name="~/Views/DashBoard/ChartLabel.cshtml" />
        </div>
    </div>
    <div class="row-main-right">

        @if (UserId != "62")
        {
            @*
            <div class="wrap_bg mb20">
                <h3 class="txt_14 line-bottom">Đơn mua hàng thành công</h3>
                <partial name="~/Views/DashBoard/SuccessOrdersList.cshtml" />

            </div>
            *@
          <partial name="~/Views/DashBoard/LogActivityToday.cshtml" />

           
         }
        <div class="wrap_bg mb20">
            <partial name="~/Views/DashBoard/RecentActivity.cshtml" />
        </div>
    </div>
</div>
@section scripts {
    <script type="text/javascript" src="~/lib/apexchart/apexcharts.js"></script>
    <script type="text/javascript" src="~/lib/Chart/Chart.js"></script>
    <script type="text/javascript" src="~/lib/Chart/Chart.PieceLabel.js"></script>
    <script src="~/modules/dashBoard.js"></script>
}
